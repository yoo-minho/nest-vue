{
  "name": "nest-vue",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production pm2 restart dist/main.js",
    "stop:prod": "pm2 stop dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:local": "dotenv -e ./config/development.env -- npm run db:migrate & dotenv -e ./config/development.env -- npm run db:sync & dotenv -e ./config/development.env -- npm run db:erd",
    "db:dev": "dotenv -e src/config/development.env -- npm run db:migrate & dotenv -e src/config/development.env -- npm run db:sync & dotenv -e src/config/development.env -- npm run db:erd",
    "db:prod": "dotenv -e src/config/production.env -- npm run db:migrate & dotenv -e src/config/production.env -- npm run db:sync & dotenv -e src/config/production.env -- npm run db:erd",
    "db:migrate": "npx prisma migrate dev --name init",
    "db:sync": "npx prisma db pull",
    "db:erd": "npx prisma generate",
    "db:seed": "npx ts-node prisma/seed.ts",
    "db:reset": "npx prisma migrate reset"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.8",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.2.1",
    "@prisma/client": "^4.1.1",
    "@types/passport-google-oauth20": "^2.0.11",
    "cache-manager": "^4.1.0",
    "cheerio": "^1.0.0-rc.11",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.4",
    "device-detector-js": "^3.0.3",
    "dotenv-cli": "^6.0.0",
    "joi": "^17.6.4",
    "node-html-parser": "^6.1.5",
    "open-graph-scraper": "^4.11.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-kakao": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-naver": "^1.0.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rss-to-json": "^2.0.2",
    "rxjs": "^7.2.0",
    "sql-formatter": "^8.2.0",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^9.1.3",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/cache-manager": "^4.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "^16.11.57",
    "@types/open-graph-scraper": "^4.8.1",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-kakao": "^1.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "prisma": "^4.1.1",
    "prisma-erd-generator": "^1.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
